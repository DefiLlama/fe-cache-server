		"@types/styled-components": "^5.1.25",
		"@types/swagger-ui-react": "^4.11.0",
		"eslint": "8.6.0",
		"eslint-config-next": "^12.3.1",
		"eslint-config-prettier": "^8.3.0",
		"prettier": "^2.5.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "4.7.4"
	},
	"_moduleAliases": {
		"~": "../src/*"
	},
	"resolutions": {
